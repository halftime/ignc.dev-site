{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      },
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          "Path": "/etc/ssl/ignc.dev/fullchain.pem",
          "KeyPath": "/etc/ssl/ignc.dev/privkey.pem"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Debug": {
    "Kestrel": {
      "Endpoints": {
        "Http": {
          "Url": "http://*:5000"
        }
      }
    }
  }
}