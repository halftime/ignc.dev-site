{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:4040"
      },
      "Https": {
        "Url": "https://*:4041",
        "Certificate": {
          "Path": "/app/certs/cloudflare-origin.pem",
          "KeyPath": "/app/certs/cloudflare-origin.key"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}